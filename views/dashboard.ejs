<main>
    <h2>Admin Dashboard</h2>
    <a href="/admin/add">Add New Article</a>
    <ul>
        <% articles.forEach(article => { %>
            <li>
                <h3><%= article.title %></h3>
                <small>Published on: <%= article.date %></small>
                <a href="/admin/edit/<%= article.id %>">Edit</a> | 
                <a href="/admin/delete/<%= article.id %>">Delete</a>
            </li>
        <% }) %>
    </ul>
</main>
